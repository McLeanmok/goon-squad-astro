---
import logo from"../static/GS-Logo.svg";
import { Image } from "astro:assets";
import Nav from "./Nav.astro";
import PrimaryBtn from "./PrimaryBtn.astro";
---

<header class="p-200">
    <div class="container">
      <div class="flex space-between content-center">
        <div class="logo">
          <a href="index.html">
            <Image
              src={logo}
              alt="Goon Squad Logo"
              title="Goon Squad logo"
          /></a>
        </div>
        <Nav />
        <div class="cta">
          <PrimaryBtn text="Join the Squad" link="/gold-cure" />
        </div>
      </div>
    </div>
  </header>